<h2>transactions</h2>
<div class="row g-3">
  <% for (let tra of transactions) { %>

  <div class="col-4">
    <div class="card p-1">
      <h5 class="card-title"><%= tra.stock_symbol %></h5>
      <div class="card-text">
        <div><%= tra.timestamp %></div>
        <div><%= tra.quantity %></div>
        <div><%= tra.type_of_trade %></div>
        <div><%= tra.price %> Citations</div>
        <div><%= tra.trader_id %></div>
        <div>
          <a
            class="btn btn-outline-primary"
            href="<%= '/transactions/' + tra.transaction_id + '/edit' %>"
            >Edit</a
          >
          <a
            class="btn btn-outline-danger"
            href="<%= '/transactions/' + tra.transaction_id + '/delete' %>"
            >Delete</a
          >
        </div>
      </div>
    </div>
  </div>

  <% } %> <%- include("./pagination.ejs", { currentPage, lastPage }) %>
</div>
